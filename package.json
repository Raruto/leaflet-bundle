{
  "name": "leaflet-browserify",
  "version": "0.0.1",
  "description": "Leaflet Browserify Bundle",
  "main": "src/bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify -t [browserify-shim --global] src/app.js | uglifyjs -cm > dist/bundle.js",
    "build:2": "browserify src/app.js -o dist/bundle.js",
    "watch": "watchify src/app.js -o dist/bundle.js"
  },
  "author": "Raruto",
  "license": "GPL-3.0",
  "browserify": {
    "transform": [
      "browserify-css",
      "browserify-shim"
    ]
  },
  "browserify-css": {
    "autoInject": false,
    "minify": true,
    "rootDir": "images",
    "output": "dist/bundle.css"
  },
  "browserify-shim": {
    "leaflet": "global:L"
  },
  "devDependencies": {
    "brfs": "^2.0.2",
    "browserify": "^16.2.0",
    "browserify-css": "^0.15.0",
    "browserify-shim": "^3.8.14",
    "uglify-js-es6": "^2.8.9",
    "watchify": "^3.11.1"
  },
  "peerDependecies": {
    "leaflet": "^1.3.1"
  },
  "dependencies": {
    "@raruto/leaflet-gesture-handling": "^1.2.2",
    "@tmcw/togeojson": "^3.0.1",
    "geojson-vt": "^3.2.1",
    "google-maps": "^3.3.0",
    "interactjs": "^1.4.10",
    "jszip": "^3.2.1",
    "leaflet-control-layers-inline": "0.0.2",
    "leaflet-hash": "^0.2.1",
    "leaflet-kmz": "^0.2.0",
    "leaflet-pegman": "^0.0.8",
    "leaflet-pointable": "^0.0.3",
    "leaflet-transparency": "^0.0.3",
    "leaflet.fullscreen": "^1.5.1",
    "leaflet.gridlayer.googlemutant": "^0.7.1",
    "leaflet.locatecontrol": "^0.67.0"
  }
}
